{
    "name": "@aws-js-sdk-v3-prerelease/client-codecommit-node",
    "description": "Node SDK for AWS CodeCommit",
    "version": "0.1.0-preview.2",
    "scripts": {
        "clean": "npm run remove-definitions && npm run remove-maps && npm run remove-js",
        "build-documentation": "npm run clean && typedoc ./",
        "prepublishOnly": "tsc",
        "pretest": "tsc",
        "remove-definitions": "rimraf *.d.ts && rimraf ./commands/*.d.ts && rimraf ./model/*.d.ts rimraf ./types/*.d.ts",
        "remove-documentation": "rimraf ./docs",
        "remove-js": "rimraf *.js && rimraf ./commands/*.js && rimraf ./model/*.js && rimraf ./types/*.js",
        "remove-maps": "rimraf *.js.map && rimraf ./commands/*.js.map && rimraf ./model/*.js.map && rimraf ./types/*.js.map",
        "test": "exit 0",
        "smoke-test": "npm run pretest && node ./test/smoke/index.spec.js"
    },
    "main": "./index.js",
    "types": "./index.d.ts",
    "author": {
        "name": "AWS SDK for JavaScript Team",
        "email": "aws-sdk-js@amazon.com",
        "url": "https://aws.amazon.com/javascript/"
    },
    "license": "Apache-2.0",
    "dependencies": {
        "@aws-js-sdk-v3-prerelease/config-resolver": "^0.1.0-preview.2",
        "@aws-js-sdk-v3-prerelease/core-handler": "^0.1.0-preview.2",
        "@aws-js-sdk-v3-prerelease/credential-provider-node": "^0.1.0-preview.2",
        "@aws-js-sdk-v3-prerelease/hash-node": "^0.1.0-preview.2",
        "@aws-js-sdk-v3-prerelease/json-builder": "^0.1.0-preview.2",
        "@aws-js-sdk-v3-prerelease/json-error-unmarshaller": "^0.1.0-preview.2",
        "@aws-js-sdk-v3-prerelease/json-parser": "^0.1.0-preview.2",
        "@aws-js-sdk-v3-prerelease/middleware-content-length": "^0.1.0-preview.2",
        "@aws-js-sdk-v3-prerelease/middleware-serializer": "^0.1.0-preview.2",
        "@aws-js-sdk-v3-prerelease/middleware-stack": "^0.1.0-preview.2",
        "@aws-js-sdk-v3-prerelease/node-http-handler": "^0.1.0-preview.2",
        "@aws-js-sdk-v3-prerelease/protocol-json-rpc": "^0.1.0-preview.2",
        "@aws-js-sdk-v3-prerelease/region-provider": "^0.1.0-preview.2",
        "@aws-js-sdk-v3-prerelease/retry-middleware": "^0.1.0-preview.2",
        "@aws-js-sdk-v3-prerelease/signature-v4": "^0.1.0-preview.2",
        "@aws-js-sdk-v3-prerelease/signing-middleware": "^0.1.0-preview.2",
        "@aws-js-sdk-v3-prerelease/stream-collector-node": "^0.1.0-preview.2",
        "@aws-js-sdk-v3-prerelease/types": "^0.1.0-preview.2",
        "@aws-js-sdk-v3-prerelease/url-parser-node": "^0.1.0-preview.2",
        "@aws-js-sdk-v3-prerelease/util-base64-node": "^0.1.0-preview.2",
        "@aws-js-sdk-v3-prerelease/util-body-length-node": "^0.1.0-preview.2",
        "@aws-js-sdk-v3-prerelease/util-utf8-node": "^0.1.0-preview.2",
        "tslib": "^1.8.0"
    },
    "devDependencies": {
        "@aws-js-sdk-v3-prerelease/client-documentation-generator": "^0.1.0-preview.2",
        "@types/jest": "^20.0.2",
        "@types/node": "^8.10.29",
        "jest": "^20.0.4",
        "rimraf": "^2.6.2",
        "typedoc": "^0.10.0",
        "typescript": "^3.0.0"
    }
}
